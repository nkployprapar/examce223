<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>lottery check</title>
    <link rel="stylesheet" href="index.css">
</head>
<body>
    <table>
        <tr>
            <th>วันที่</th>
            <th>เลขที่ออก</th>
            <th>รางวัลที่</th>
            <th>จำนวนเงิน</th>
        </tr>
        <% lotterycheck.forEach(function(item) { %>
        <tr>
            <td><%= item.date %></td>
            <td><%= item.types %></td>
            <td><%= item.number %></td>
            <td><%= item.money %></td>
        </tr>
    <% }) %>
    </table><hr>

    <p>ตรวจสอบรางวัล</p>
    <form action="">
        <div>
            <h3>เลขของคุณ</h3>
            <input class="input" type="text" name="numberout" id="numberout">
            <button type="submit">submit</button>
        </div>
    </form>
</body>
</html>